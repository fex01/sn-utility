})(step);]]&gt;&lt;/description_generator&gt;&lt;html_description&gt;&lt;![CDATA[&lt;p&gt;&amp;lt;p&amp;gt;&lt;br /&gt;  &amp;lt;strong&amp;gt;Goal:&amp;lt;/strong&amp;gt; Retrieve the raw (non-display) value of a field from a record, given the table name, record sys_id, and field name.&lt;br /&gt;&amp;lt;/p&amp;gt;&lt;br /&gt;&amp;lt;p&amp;gt;&lt;br /&gt;  &amp;lt;strong&amp;gt;Example Use Case:&amp;lt;/strong&amp;gt; Use this step to fetch values for further assertions or use in parameterized tests—such as comparing reference IDs, numeric, or string values in subsequent steps.&lt;br /&gt;&amp;lt;/p&amp;gt;&lt;br /&gt;&amp;lt;p&amp;gt;&lt;br /&gt;  &amp;lt;strong&amp;gt;How to use:&amp;lt;/strong&amp;gt;&lt;br /&gt;  &amp;lt;ul&amp;gt;&lt;br /&gt;    &amp;lt;li&amp;gt;Provide the technical table name (e.g., &amp;lt;code&amp;gt;x_app_vehicle_model&amp;lt;/code&amp;gt;).&amp;lt;/li&amp;gt;&lt;br /&gt;    &amp;lt;li&amp;gt;Provide the sys_id of the record to inspect.&amp;lt;/li&amp;gt;&lt;br /&gt;    &amp;lt;li&amp;gt;Specify the field&amp;#39;s technical name (e.g., &amp;lt;code&amp;gt;status&amp;lt;/code&amp;gt; or &amp;lt;code&amp;gt;assigned_to&amp;lt;/code&amp;gt;).&amp;lt;/li&amp;gt;&lt;br /&gt;    &amp;lt;li&amp;gt;The step will output the field&amp;#39;s raw value as a string.&amp;lt;/li&amp;gt;&lt;br /&gt;  &amp;lt;/ul&amp;gt;&lt;br /&gt;&amp;lt;/p&amp;gt;&lt;/p&gt;]]&gt;&lt;/html_description&gt;&lt;icon&gt;icon-article-document&lt;/icon&gt;&lt;inputs/&gt;&lt;name&gt;Get Field Value&lt;/name&gt;&lt;order&gt;5700&lt;/order&gt;&lt;outputs/&gt;&lt;performance_include&gt;false&lt;/performance_include&gt;&lt;step_env display_value="Server - Independent"&gt;6c2bcea1870312009dccc9ded0e3ecca&lt;/step_env&gt;&lt;step_execution_generator&gt;&lt;![CDATA[(function(inputs, outputs, stepResult) {
    try {
        var tableName = inputs.u_table_name &amp;&amp; inputs.u_table_name.trim();
        var recordSysId = inputs.u_record_sys_id &amp;&amp; inputs.u_record_sys_id.trim();
        var fieldName = inputs.u_field_name &amp;&amp; inputs.u_field_name.trim();

        if (!tableName || !recordSysId || !fieldName) {
            stepResult.setOutputMessage("Missing required input(s):" +
                (!tableName ? " Table Name;" : "") +
                (!recordSysId ? " Record Sys ID;" : "") +
                (!fieldName ? " Field Name;" : "")
            );
            stepResult.setFailed();
            return;
        }

        // Table and field validation
        var td = GlideTableDescriptor.get(tableName);
        if (!td || !td.isValid()) {
            stepResult.setOutputMessage("Table '" + tableName + "' does not exist or is not accessible.");
            stepResult.setFailed();
            return;
        }

        var gr = new GlideRecord(tableName);
        if (!gr.isValidField(fieldName)) {
            stepResult.setOutputMessage("Field '" + fieldName + "' does not exist on table '" + tableName + "'.");
            stepResult.setFailed();
            return;
        }

        if (!gr.get(recordSysId)) {
            stepResult.setOutputMessage("Record with sys_id '" + recordSysId + "' not found in table '" + tableName + "'.");
            stepResult.setFailed();
            return;
        }

        var value = gr.getValue(fieldName);
        outputs.u_field_value = value;

        stepResult.setOutputMessage("Field '" + fieldName + "' value for record '" + recordSysId + "' in table '" + tableName + "': " + value);
        stepResult.setSuccess();
    } catch (e) {
        stepResult.setOutputMessage("Unexpected error: " + e.message);
        stepResult.setFailed();
    }
})(inputs, outputs, stepResult);]]&gt;&lt;/step_execution_generator&gt;&lt;sys_class_name&gt;sys_atf_step_config&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2025-07-24 09:56:31&lt;/sys_created_on&gt;&lt;sys_id&gt;a1ce747fc33622103a2ebfddd40131d7&lt;/sys_id&gt;&lt;sys_mod_count&gt;0&lt;/sys_mod_count&gt;&lt;sys_name&gt;Get Field Value&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_atf_step_config_a1ce747fc33622103a2ebfddd40131d7&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2025-07-24 09:56:31&lt;/sys_updated_on&gt;&lt;template_reminder&gt;Get the value of a field from a record by table name, sys_id, and field name.&lt;/template_reminder&gt;&lt;type display_value="Test Step" name="sys_atf_step"&gt;35ff02f778e022104f3459503ff42af0&lt;/type&gt;&lt;wizard_redirect/&gt;&lt;/sys_atf_step_config&gt;&lt;sys_translated_text action="delete_multiple" query="documentkey=a1ce747fc33622103a2ebfddd40131d7"/&gt;&lt;sys_variable_value action="delete_multiple" query="document_key=a1ce747fc33622103a2ebfddd40131d7"/&gt;&lt;sys_element_mapping action="delete_multiple" query="table=var__m_atf_input_variable_a1ce747fc33622103a2ebfddd40131d7^id=a1ce747fc33622103a2ebfddd40131d7"/&gt;&lt;sys_element_mapping action="delete_multiple" query="table=var__m_atf_output_variable_a1ce747fc33622103a2ebfddd40131d7^id=a1ce747fc33622103a2ebfddd40131d7"/&gt;&lt;sys_es_latest_script action="INSERT_OR_UPDATE"&gt;&lt;id&gt;a1ce747fc33622103a2ebfddd40131d7&lt;/id&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2025-07-24 09:56:30&lt;/sys_created_on&gt;&lt;sys_id&gt;9c5f347fc33622103a2ebfddd40131df&lt;/sys_id&gt;&lt;sys_mod_count&gt;0&lt;/sys_mod_count&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;